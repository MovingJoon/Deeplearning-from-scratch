        "##활성화 함수\n",
        "#시그모이드 함수 \n",
        "def sigmoid(x):\n",
        "  return 1/(1+np.exp(-x))\n",
        "\n",
        "#계단함수 \n",
        "def step_function(x):\n",
        "  y=x>0\n",
        "  return y.astype(np.int)\n",
        "\n",
        "#Relu함수 \n",
        "def Relu(x):\n",
        "  return np.maximum(0,x)"
