        "#소프트맥스 함수 \n",
        "def softmax(x):\n",
        "  c=np.max(x)\n",
        "  exp_a = np.exp(x-c)\n",
        "  exp_a_sum=np.sum(exp_a)\n",
        "  y=exp_a/exp_a_sum\n",
        "  \n",
        "  return y"
      ],
